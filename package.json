{
  "name": "karin-plugin-mystool",
  "version": "1.0.0",
  "author": "babanbang",
  "type": "module",
  "description": "karin plugin MysTool",
  "homepage": "https://github.com/babanbang/karin-plugin-MysTool",
  "bugs": {
    "url": "https://github.com/babanbang/karin-plugin-MysTool/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/babanbang/karin-plugin-MysTool.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "addpkg": "node pkg.js --type add",
    "delpkg": "node pkg.js --type del",
    "init": "npm run addpkg && npm install",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && npm run delpkg",
    "pub": "npm publish --access public"
  },
  "dependencies": {
    "sequelize": "^6.37.3",
    "sqlite3": "5.1.6",
    "pg": "^8.12.0",
    "md5": "^2.3.0",
    "image-size": "^1.1.1"
  },
  "karin": {
    "apps": [
      "lib/apps"
    ],
    "outDir": "lib",
    "rootDir": "src"
  },
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "/config/*.yaml",
    "/config/*.js",
    "resources",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}