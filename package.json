{
  "name": "karin-plugin-MysTool",
  "version": "1.0.0",
  "author": "babanbang",
  "type": "module",
  "description": "karin plugin template",
  "homepage": "https://github.com/babanbang/karin-plugin-MysTool",
  "bugs": {
    "url": "https://github.com/babanbang/karin-plugin-MysTool/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/babanbang/karin-plugin-MysTool.git"
  },
  "scripts": {
    "addpkg": "node pkg.js --type add",
    "delpkg": "node pkg.js --type del",
    "init": "npm run addpkg && npm install",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && npm run delpkg",
    "pub": "npm publish --access public"
  },
  "dependencies": {
    "sequelize": "^6.37.3",
    "sqlite3": "5.1.6",
    "pg": "^8.12.0",
    "md5": "^2.3.0",
    "qrcode": "^1.5.3",
    "image-size": "^1.1.1"
  },
  "karin": {
    "apps": [
      "lib/apps",
      "lib/components/Genshin/Apps",
      "lib/components/StarRail/Apps",
      "lib/components/ZZZero/Apps",
      "lib/components/MysSign/Apps"
    ],
    "outDir": "lib",
    "rootDir": "src"
  },
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "/config/*.yaml",
    "resources",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}